        "laravel-mix": "^1.0",
        "lodash": "^4.17.5",
        "vue": "^2.5.16",
        "vue-router": "^3.0.1"
    },
    "dependencies": {
        "chart.js": "^2.7.2",
        "clipboard": "^2.0.0",
